<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.my_first_database"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="Intranet"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.my_first_database.MainActivity"
            android:label="Intranet" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.my_first_database.MainActivity1"
            android:label="Home Page" >
        </activity>
        <activity
            android:name="com.example.my_first_database.MainActivity2"
            android:label="Time Table" >
        </activity>
        <activity
            android:name="com.example.my_first_database.MainActivity3"
            android:label="Activities" >
        </activity>
    </application>

</manifest>
